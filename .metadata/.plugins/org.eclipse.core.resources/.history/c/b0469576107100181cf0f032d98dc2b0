package co.megadodo.terraingen1.gen.mine1;

import java.util.ArrayList;

import co.megadodo.terraingen1.block.BlockState;

//An abstract biome
//co.megadodo.terraingen1.gen.mine1 is not really used
//but still pretty cool
public abstract class AbstractBiome {
	
	//This is not used
	@Deprecated
	public ArrayList<BlockPair> spawnDecos(int x,int y,int z) {
		return new ArrayList<BlockPair>();
	}
	
	public abstract BlockState[] paintColumn(int x, int z);

}
