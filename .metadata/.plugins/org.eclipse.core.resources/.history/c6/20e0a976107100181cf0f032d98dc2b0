package co.megadodo.terraingen1.appstate;


//A raytraceresult
//A wrapper for 4 vectors:
//hitInChunk: (hitX,hitY,hitZ)
//hitChunk: (hitCX,hitCZ)
//lastInChunk: (lX,lY,lZ)
//lastChunk: (lCX,lCZ)
public class RayTraceResult {

	
	public int hitX,hitY,hitZ,hitCX,hitCZ;
	public int lX,lY,lZ,lCX,lCZ;

}
