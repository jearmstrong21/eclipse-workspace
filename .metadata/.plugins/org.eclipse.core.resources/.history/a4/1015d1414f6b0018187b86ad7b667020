	public static void main(String[] args) {
		new Mathy();
	}

	public Mathy() {
		JFrame frame = new JFrame("Mathy calculator");
		frame.setSize(1000, 1000);
		
		PanelProvider pp;
		
		frame.add(pp.getPanel());

