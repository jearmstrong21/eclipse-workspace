package co.megadodo.voronoidelaunay;

public class Triangle {
	
	public Vector2f a;
	public Vector2f b;
	public Vector2f c;
	
	public Triangle() {
		
	}
	
	public Line lineAB() {
		return new Line(a, b);
	}
	
	public Line lineBC() {
		return new Line(b, c);
	}
	
	public Line lineAC() {
		return new Line(a, c);
	}
	
	public Vector2f circumcenter() {
		return Line.intersection(lineAB(), lineBC());
	}

}
