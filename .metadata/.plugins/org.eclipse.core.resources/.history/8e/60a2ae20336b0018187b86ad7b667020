package co.megadodo.lwjglswingshaderfun;

import java.util.ArrayList;

import static org.lwjgl.opengl.GL20.*;


public class VertexBuffer {
	
	public int vbo;
	public int vao;
	public int ebo;
	
	public VertexBuffer() {
		
	}
	
	public void genBuffers(ArrayList<Vertex>list) {
		glGenVertexArray();
	}

}
